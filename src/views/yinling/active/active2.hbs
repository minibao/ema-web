<div class="frm">
    <div class="top">
        <div class="list">
            <div class="big-prize">大奖记录</div>
            <div id="andyscroll">
                <div id="scrollmessage">
                    <ul>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="fuli">
        <div class="big-prize">福利区</div>
        <div class="fanpai">
            <div id="containerfanpai"></div>
        </div>
    </div>

    <div @click="chouwanxipai" class="center cl-xipai" style="">
        <div>夺宝</div>
        <div class="cl-xipai-btn">每次消耗288柠檬点</div>
    </div>

</div>

<div id="gainingmask3" class="showimgBig">
    <div class="gbox">
        <div style="padding: 0%;padding-top: 3%;" class="g-word">
            <span id="showimgBigName"></span>
            <span style="padding: 0px;" id="tks">
                <img style="max-width: 60%;max-height: 60%;margin-top: 0px;" src="">
            </span>
        </div>
        <div style="bottom: 60px;width: 120px;margin-left: -60px;background: -moz-linear-gradient(top, #a128ff, #5816ff); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#a128ff), to(#5816ff));" class="gbtn3">再次夺宝</div>
        <div class="two-btn">
            <div class="left">
                <div style="-moz-linear-gradient(top, #f2c551, #e6942c); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f2c551), to(#e6942c));">炫耀一下</div>
            </div>
            <div class="right">
                <div id="gotoprize" style="-moz-linear-gradient(top, #f68f8b, #ee4e4c); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f68f8b), to(#ee4e4c));">前去领奖</div>
            </div>
        </div>
    </div>
</div>

<div id="gainingmask1" class="showimgBig">
    <div class="gbox">
        <div style="padding: 8% 5% 15% 5%;" class="g-word">
            <span style="padding:0px" id="showimgBigName1"></span>
            <span style="padding:0px" id="tks"><img style="max-width: 60%;max-height: 60%;" src=""></span>
        </div>
        <div style="bottom: 60px;width: 120px;margin-left: -60px;" class="gbtn3 gbtn3New">再次夺宝</div>
        <div style="bottom: 15px;width: 120px;margin-left: -60px;" class="gbtn4">炫耀一下</div>
    </div>
</div>

